{
  "name": "yololiv-ultra",
  "version": "0.1.12-beta.0",
  "description": "Bitfocus Companion module for controlling a YoloLiv YoloBox Ultra via its Web Control websocket API.",
  "main": "main.js",
  "scripts": {
    "format": "prettier -w .",
    "package": "companion-module-build",
    "package:beta": "companion-module-build --prerelease",
    "bump": "node scripts/bump-version.js",
    "release": "npm run bump -- patch && npm run package",
    "release:beta": "npm run bump -- beta && npm run package:beta"
  },
  "author": "Massimo Saccani",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/supergiovane/companion-to-yolobox.git"
  },
  "homepage": "https://github.com/supergiovane/companion-to-yolobox#readme",
  "bugs": {
    "url": "https://github.com/supergiovane/companion-to-yolobox/issues"
  },
  "keywords": [
    "companion-module",
    "yololiv",
    "yolobox",
    "streaming"
  ],
  "dependencies": {
    "@companion-module/base": "~1.13.4",
    "ajv": "^8.17.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@companion-module/tools": "^2.4.2",
    "prettier": "^3.3.3",
    "semver": "^7.6.3"
  },
  "prettier": "@companion-module/tools/.prettierrc.json",
  "packageManager": "yarn@4.10.3"
}
